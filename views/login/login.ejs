<% include ../header %>
    <div id="mainlogin">
        <div class="container ">
            <div class="container">
                <div class="col-sm-6 col-sm-offset-3 formlogin1 bodorformlogin">
                    <div style="margin-top: 30px">
                        <div style="padding-left: 5px;">
                            <form id="form-login" onsubmit="return false">
                                <h3 class="fontsingin">Sign In</h3>
                                <div class="form-group has-feedback">
                                    <label class="control-label" for="inputSuccess2">Username *</label>
                                    <input type="text" name="username" class="resetinputlogin" aria-describedby="inputSuccess2Status">
                                    <span class="form-control-feedback" aria-hidden="true">
                                        <img class="seticoneye iconerr icon" id="iconerr1" src="" alt="">
                                    </span>
                                </div>
                                <div class="form-group has-feedback">
                                    <label class="control-label" for="inputSuccess2">Password *</label>
                                    <input type="password" name="pass" class="resetinputlogin" aria-describedby="inputSuccess2Status">
                                    <span class="form-control-feedback" aria-hidden="true">
                                        <img class="seticoneye iconerr" id="iconeye" src="/themes/img/login/iconeye1.png" alt="">
                                        <img class="seticoneye iconerr" id="iconeyehide" src="/themes/img/login/iconeyehide.png" alt="">
                                    </span>
                                </div>
                                <div class="form-group-12" id="remember1">
                                    <div class="checkboxFive">
                                        <div id="remem">
                                            <input type="checkbox" value="faile" id="rememberme">
                                            <label for="rememberme"></label>
                                            <span class="spanremember">&nbsp Remember me</span>
                                        </div>
                                        <div id="forgot1">

                                            <a class="setcolorfont setaforgotdefaut" data-toggle="modal" data-target="#myModal" style="float: right">Forgot Password?</a>

                                        </div>
                                    </div>
                                </div>
                                <br>
                                <br>
                                <input type="submit" id="btn-login" class="btn setcolorfontinput setcolorloginbackround btn-block" value="Login">
                                <br>
                                <div class=" textcenter form-group-edit">
                                    Or Sign In with:&nbsp&nbsp
                                    <a class="setaauth" href="/auth/fb">
                                        <img src="/themes/img/login/iconfb1.png" alt="">
                                    </a> &nbsp
                                    <a class="setaauth" href="/auth/gg">
                                        <img src="/themes/img/login/icongg.png" alt="">
                                    </a> &nbsp
                                    <a class="setaauth" href="/auth/tw">
                                        <img src="/themes/img/login/icontw.png" alt="">
                                    </a>
                                </div>
                                <br>
                                <div class="textcenter form-group settextifyou">
                                    If you do not already have an account.
                                    <a class="setcolorfont" href="/register">Register now</a>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <div class="container ">
                <div class="modal fade" id="myModal" role="dialog">
                    <div class="modal-dialog  ">
                        <div class="hidden-xs" style="margin-top: 130px"></div>
                        <div class="visible-xs-*" style="margin-top: 70px"></div>
                        <div class="modal-content ">
                            <button type="button" class="close btnclose" data-dismiss="modal" aria-label="Close">
                                <span class="setxclose" aria-hidden="true">&times;</span>
                            </button>
                            <div class="setdivmodal">
                                <div class="form-group textcenter">
                                    <img src="/themes/img/login/iconchamhoi1.png" alt="">
                                </div>
                                <div class="form-group textcenter">
                                    <h2 class="fontforgot">Forgot Your password?</h2>
                                    <span class="font-forgotmail">
                                        Please enter your email address below and we will email you with instructions in how to reset your password
                                    </span>
                                </div>
                                <form id="form-forgotpass" onsubmit="return false">
                                    <div class="form-group1 has-feedback">
                                        <label class="control-label" for="inputSuccess2"></label>
                                        <input type="text" class="resetinputlogin" name="emailresetpass" aria-describedby="inputSuccess2Status" placeholder="Email address">
                                        <span class="form-control-feedback1" aria-hidden="true">
                                            <img class="seticonemail" name="iconemail" src="/themes/img/login/iconemail.png" alt="">
                                        </span>
                                    </div>
                                    <div class="textcenter form-group2">
                                        <button type="submit" id="btn-resetpass" class="btn setcolorfontinput btn-lg-forgot setcolorloginbackround ">
                                            <img src="/themes/img/login/iconbutton.png"> Send</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <% include ../footer%>