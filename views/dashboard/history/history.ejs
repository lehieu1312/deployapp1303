                        </th>
                    </tr>
                    <%for(var i = 0; i<history.length ;i++){ %>
                        <% var number = history.length - i - 1;
                        var d = history[number].dateCreate;
                        var m = d.getMonth() + 1;
                        var updated = d.getDate() + "/" + m + "/" + d.getFullYear() + " " + d.getHours() + ":" + d.getMinutes() + ":" + d.getSeconds() + " GMT";    
                        var arr = updated.split(" ");
                         %>
                            <tr id="version<%=i%>" class="tr-content-appversion findcode">
                                <td>
                                    <input id="history-app" hidden value="<%=history[number].idApp%>">
                                    <%= i + 1%>
                                </td>
                                <td id="nameapp-table" class="table-time-appversion finddate">
                                    <input id="time-ready<%=i%>" type="hidden" value="<%=d%>">
                                    <span>
                                        <%=arr[0]%>
                                    </span>
                                    <br>
                                    <span>
                                        <%=arr[1]%>
                                            <%=arr[2]%>
                                    </span>
                                </td>
                                <td class="table-nameapp findnameapp">
                                    <%=appuse.nameApp%>
                                </td>
                                <td class="table-version findversion">
                                    <%=history[number].version%>
                                </td>
                                <td class="table-changelog findnote">
                                    <% if(history[number].note != ""){ %>
                                        <span class="note-table-history">
                                            <%=history[number].note%>
                                        </span>
                                        <br>
                                        <a class="write-note" href="#" data-toggle="modal" data-target="#myModal">
                                            <img src="/themes/img/appversion/iconwrite.png" atl="iconwrite"> Edit
                                        </a>
                                        <%  }else{%>
                                            <!-- <span class="note-table-history">
                                            </span> -->
                                            <a class="write-note" href="#" data-toggle="modal" data-target="#myModal">
                                                <img src="/themes/img/appversion/iconwrite.png" atl="iconwrite"> Write</a>
                                            <%  }%>
                                </td>
                                <!-- <td>
                                    <button id="btn-deploy">DEPLOY</button>
                                </td> -->
                            </tr>
                            <%}%>

                                </tbody>
                </table>

