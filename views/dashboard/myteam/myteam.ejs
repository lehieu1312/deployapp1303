                    </tr>
                    <tbody>
                        <%for(var i = 0;i<myteam.length;i++){ %>
                            <%var d = myteam[i].dateAdded%>
                                <%var m = d.getMonth() + 1%>
                                    <%var updated = d.getDate() + "/" + m + "/" + d.getFullYear() + " " + d.getHours() + ":" + d.getMinutes() + ":" + d.getSeconds() + " GMT";%>
                                        <%var arr = updated.split(" ")%>
                                            <tr id="version<%=i%>" class="tr-content-appversion">
                                                <td class="findcode">
                                                    <input id="idapp-using" hidden value="<%=appuse.idApp%>">
                                                    <%= i + 1%>
                                                </td>
                                                <td class="table-nameapp findversion">
                                                    <%if(myteam[i].picture == null){%>
                                                        <img class="imgavatarsmall1" src="/themes/img/dashboard/Avatar.png" alt="myteam">
                                                        <%}else{%>
                                                            <img class="imgavatarsmall1" src="/themes/img/profile/<%=myteam[i].picture%>" s alt="myteam">
                                                            <%}%>
                                                </td>
                                                <td class="table-version findnameapp">
                                                    <%=myteam[i].name%>
                                                </td>
                                                <td class="table-changelog findnote">
                                                    <span class="text-email">
                                                        <%=myteam[i].email%>
                                                    </span>
                                                </td>
                                                <td class="table-time-appversion finddate">
                                                    <input id="time-ready<%=i%>" type="hidden" value="<%=d%>">
                                                    <span>
                                                        <%=arr[0]%>
                                                    </span>
                                                    <br>
                                                    <span>
                                                        <%=arr[1]%>
                                                            <%=arr[2]%>
                                                    </span>
                                                </td>
                                                <td class="set-position-relative">
                                                    <img class="setimg-head-myapp deleteuser" src="/themes/img/dashboard/iconmenumore.png">
                                                    <div class="delete-user" style="display:none">
                                                        <a href="#" data-toggle="modal" data-target="#mymodal-deleteuser">
                                                            <div class="element-delete">
                                                                <img class="imgicondelete" src="/themes/img/dashboard/icondelete.png">
                                                                <span>Delete</span>
                                                            </div>
                                                        </a>
                                                        <div class="arrowmenuuser-left">
                                                        </div>
                                                        <div class="arrowmenuuser-left1">
                                                        </div>
                                                    </div>
                                                </td>
                                            </tr>
                                            <%}%>
