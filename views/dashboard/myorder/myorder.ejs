        <%
            var Pending = 0;
            var Processing = 0;
            var onhold = 0;
            var Completed = 0;
            var Cancelled = 0;
            var Refunded = 0;
            var Failed = 0;
            for(let i=0;i<myorder.length;i++){
                if(myorder[i].order.statusOrder == 1){
                    Pending++;
                }
                if(myorder[i].order.statusOrder == 2){
                    Processing++;
                }
                if(myorder[i].order.statusOrder == 3){
                    onhold++;
                }
                if(myorder[i].order.statusOrder == 4){
                    Completed++;
                }
                if(myorder[i].order.statusOrder == 5){
                    Cancelled++;
                } 
                if(myorder[i].order.statusOrder == 6){
                    Refunded++;
                }
                if(myorder[i].order.statusOrder == 7){
                    Failed++;
                }
            }
