<%include ../header.ejs%>

    <div id="track-info" class="container">

        <h1>Infomations </h1>
        <b> Uploaded:
        <%=sUploaded%></b> <br/><b> Deployed:
        <%=sDeployed%>
            </b><br/>

        <table id="data-track" class="table table-striped table-hover results">
            <thead>
                <tr>
                    <th>N.o</th>
                    <th>App Name</th>
                    <th>File APK debug</th>
                    <th>Email</th>
                    <th>Platforms</th>
                    <th>Step</th>
                    <th>Date create</th>
                </tr>
            </thead>
            <tbody>
                <% var dem=1;
                         sInfomations.forEach(function(info){%>
                    <tr>
                        <th scope="row">
                            <%=dem%>
                        </th>
                        <td style="word-wrap: break-word;">
                            <%= info.appName %>
                        </td>
                        <td style="word-wrap: break-word;">

                            <a href="http://deployapp.net/platforms/<%=info.keyFolder%>" target="_blank">
                                      File APK  <%= info.appName %>
                                    </a>
                        </td>
                        <td>
                            <%= info.email %>
                        </td>
                        <td>
                            <%= info.platforms %>
                        </td>
                        <td>
                            <%= info.stepBuild %>
                        </td>
                        <td>
                            <%=moment(info.dateCreate).format('YYYY-MM-DD HH:mm:ss')  %>
                        </td>
                    </tr>
                    <% dem++;%>

                        <%})%>
            </tbody>
        </table>
