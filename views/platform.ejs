<% include header%>

    <div class="container">
        <div class="stepProcess">
            <ul class="steps">
                <li class="step completed">
                    <div class="step-info">
                        <span class="step-name">Upload Source</span>
                    </div>
                </li>

                <li class="step completed">
                    <div class="step-info">
                        <span class="step-name">Setting Your App</span>
                    </div>
                </li>
                <li class="step active">
                    <div class="step-info">
                        <span class="step-name">Choose Platforms</span>
                    </div>
                </li>
                <li class="step">
                    <div class="step-info">
                        <span class="step-name">Customization</span>
                    </div>
                </li>

            </ul>
        </div>
        <div class="pl-panel">
            <input type="hidden" id="platformval" />
            <input type="hidden" id="aKey" value="<%=fKeyFolder%>" />
            <div class="row head-platform">
                <h1 class="font-bold chose-platform-text">Chose Platforms</h1>
                <!-- <p class="textinfo-choseplatform">You are about to get your app done</p> -->
            </div>

            <div class="row content-platform">
                <div class="row-platform">
                    <div class="col-xs-12 col-md-6 box-platform-android">
                        <a href="javascript:void(0);" id="android" class="">
                            <div class="row pl-android">
                                <div class="col-xs-10 platform-left padding-0">
                                    <div class="tile-pl-android text-tile-platform">Android</div>
                                    <div class="text-info-platform">Build your project for testing on android device</div>
                                </div>
                                <div class="col-xs-2 padding-0">
                                    <img class="img-responsive icon-row-platform" alt="android platforms" src="data:image/svg+xml;base64,"
                                    />
                                </div>
                            </div>

                        </a>
                    </div>

                    <div class="col-xs-12 col-md-6 box-platform-ios">
                        <a href="javascript:void(0);" id="ios" class="">

                            <div class="row pl-ios">
                                <div class="col-xs-10 platform-left padding-0">
                                    <div class="tile-pl-ios text-tile-platform">iOS</div>
                                    <div class="text-info-platform">Build your project for testing on iOS device</div>
                                </div>
                                <div class="col-xs-2 padding-0">
                                    <img class="img-responsive icon-row-platform" alt="apple platforms" src="data:image/svg+xml;base64,"
                                    />
                                </div>
                            </div>

                        </a>

                    </div>
                </div>

            </div>


            <div class="row">
                <div class="row footer-platform">
                    <button type="button" class="btn btn-lg btn-platform" id="btn-send-platform">NEXT â†’</button>
                </div>
            </div>
        </div>


    </div>

    <% include footer%>