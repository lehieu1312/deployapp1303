                                        <label class="control-label"> Alias name *</label>
                                        <div class="">
                                            <input type="text" class="form-control input-form-info" id="alias_name" placeholder="" displaytext="Alias" name="alias" value="" />
                                            <span id="icon-err-alias-name" class="glyphicon glyphicon-err form-control-feedback display-none"></span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <div class="">
                                            <!-- DEmo modal -->

                                            <!-- Close Demo Modal -->
                                            <!-- <input type="button"  value="Build android" class="btn btn-primary"> -->
                                            <!-- onclick="send_ajax_build_ios()" -->
                                            <!-- → -->
                                            <button type="button" id="btn-build-android-update" class="btn btn-lg btn-infobuild">DEPLOY</span></button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                    <!-- Close form -->

                    <!-- dff -->
                </div>

            </div>
        </div>
    </div>

